import {createSlice} from '@reduxjs/toolkit'

const initialState = '';

const searchSlice = createSlice({
  name: 'search',
  initialState,
  reducers: {
    filterChanged:{
      reducer(state, action){
        const {filter} = action.payload;
        state.filter = 'filter'
      },
      prepare({filter}){
        return {
          payload: {filter}
        }
      }
    }
  }
})

export const {filterChanged} = moviesSlice.actions;

export default searchSlice.reducer;